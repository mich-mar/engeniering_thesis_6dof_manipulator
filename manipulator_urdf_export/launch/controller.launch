<launch>

<rosparam file="$(find manipulator_final_before_export_description)/launch/controller.yaml" command="load"/>
<node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen" ns="manipulator_final_before_export" args="base_to_1_position_controller 1_to_2_position_controller 2_to_3_position_controller 3_to_4_position_controller 4_to_5_position_controller Revolute 7_position_controller joint_state_controller "/>
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
  <remap from="/joint_states" to="/manipulator_final_before_export/joint_states"/>
</node>

</launch>